    ///
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The result immarray.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="distinct-1">
    /// <code lang="fsharp">
    /// let input = [| 1; 1; 2; 3 |]
    ///
    /// input |> immarray.distinct
    /// </code>
    /// Evaluates to <c>[| 1; 2; 3 |]</c>
    /// </example>
    [<CompiledName("Distinct")>]
    val distinct: array: 'T immarray -> 'T immarray when 'T: equality

    /// <summary>Returns an immarray that contains no duplicate entries according to the
    /// generic hash and equality comparisons on the keys returned by the given key-generating function.
    /// If an element occurs multiple times in the immarray then the later occurrences are discarded.</summary>
    ///
    /// <param name="projection">A function transforming the immarray items into comparable keys.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The result immarray.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="distinct-by-1">
    /// <code lang="fsharp">
    /// let inputs = [| {Bar = 1 };{Bar = 1}; {Bar = 2}; {Bar = 3} |]
    ///
    /// inputs |> immarray.distinctBy (fun foo -> foo.Bar)
    /// </code>
    /// Evaluates to <c>[| { Bar = 1 }; { Bar = 2 }; { Bar = 3 } |]</c>
    /// </example>
    [<CompiledName("DistinctBy")>]
    val distinctBy: projection: ('T -> 'Key) -> array: 'T immarray -> 'T immarray when 'Key: equality

    /// <summary>Splits the input immarray into at most <c>count</c> chunks.</summary>
    ///
    /// <param name="count">The maximum number of chunks.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The immarray split into chunks.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    /// <exception cref="T:System.ArgumentException">Thrown when <c>count</c> is not positive.</exception>
    ///
    /// <example id="split-into-1">
    /// <code lang="fsharp">
    /// let inputs = [| 1; 2; 3; 4; 5 |]
    ///
    /// inputs |> immarray.splitInto 3
    /// </code>
    /// Evaluates to <c>seq [| [|1; 2|]; [|3; 4|]; [|5|] |]</c>
    /// </example>
    ///
    /// <example id="split-into-2">
    /// <code lang="fsharp">
    /// let inputs = [| 1; 2; 3; 4; 5 |]
    ///
    /// inputs |> immarray.splitInto -1
    /// </code>
    /// Throws <c>ArgumentException</c>
    /// </example>
    [<CompiledName("SplitInto")>]
    val splitInto: count: int -> array: 'T immarray -> 'T immarray immarray

    /// <summary>Returns an empty immarray of the given type.</summary>
    /// <returns>The empty immarray.</returns>
    ///
    /// <example id="empty">
    /// <code lang="fsharp">
    /// immarray.empty // Evaluates to [| |]
    /// </code>
    /// </example>
    [<GeneralizableValue>]
    [<CompiledName("Empty")>]
    val empty<'T> : 'T immarray

    /// <summary>Returns the only element of the immarray.</summary>
    ///
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The only element of the immarray.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    /// <exception cref="T:System.ArgumentException">Thrown when the input does not have precisely one element.</exception>
    ///
    /// <example id="exactly-one-1">
    /// <code lang="fsharp">
    /// let inputs = [| "banana" |]
    ///
