namespace Microsoft.FSharp.Collections

open System
open Microsoft.FSharp.Core
open Microsoft.FSharp.Collections
open System.Collections.Immutable

type 'T immarray = ImmutableArray<'T>

/// <summary>Contains operations for working with immarrays.</summary>
///
/// <remarks>
///  See also <a href="https://learn.microsoft.com/dotnet/fsharp/language-reference/arrays">F# Language Guide - immarrays</a>.
/// </remarks>
[<CompilationRepresentation(CompilationRepresentationFlags.ModuleSuffix)>]
[<RequireQualifiedAccess>]
module ImmArray =

    /// <summary>Returns a new immarray that contains all pairings of elements from the first and second immarrays.</summary>
    ///
    /// <param name="array1">The first input immarray.</param>
    /// <param name="array2">The second input immarray.</param>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when either of the input immarrays is null.</exception>
    ///
    /// <returns>The resulting immarray of pairs.</returns>
    ///
    /// <example id="all-pairs-1">
    /// <code lang="fsharp">
    /// ([| 1; 2 |], [| 3; 4 |]) ||> immarray.allPairs
    /// </code>
    /// Evaluates to
    /// <code>
    /// [| (1, 3); (1, 4); (2, 3); (2, 4) |]
    /// </code>
    /// </example>
    [<CompiledName("AllPairs")>]
    val allPairs: array1: 'T1 immarray -> array2: 'T2 immarray -> ('T1 * 'T2) immarray

    /// <summary>Builds a new immarray that contains the elements of the first immarray followed by the elements of the second immarray.</summary>
    ///
    /// <param name="array1">The first input immarray.</param>
    /// <param name="array2">The second input immarray.</param>
    ///
    /// <returns>The resulting immarray.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when either of the input immarrays is null.</exception>
    ///
    /// <example id="append-1">
    /// <code lang="fsharp">
    /// immarray.append [| 1; 2 |] [| 3; 4 |]
    /// </code>
    /// Evaluates to <c>[| 1; 2; 3; 4 |]</c>.
    /// </example>
    [<CompiledName("Append")>]
    val append: array1: 'T immarray -> array2: 'T immarray -> 'T immarray

    /// <summary>Returns the average of the elements in the immarray.</summary>
    ///
    /// <param name="array">The input immarray.</param>
    ///
    /// <exception cref="T:System.ArgumentException">Thrown when <c>array</c> is empty.</exception>
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <returns>The average of the elements in the immarray.</returns>
    ///
    /// <example id="average-1">
    /// <code lang="fsharp">
    /// [| 1.0; 2.0; 6.0 |] |> immarray.average
    /// </code>
    /// Evaluates to <c>3.0</c>
    /// </example>
    ///
    /// <example id="average-2">
    /// <code lang="fsharp">
    /// [| |] |> immarray.average
    /// </code>
    /// Throws <c>ArgumentException</c>
    /// </example>
    [<CompiledName("Average")>]
    val inline average:
        array: ^T immarray -> ^T
            when ^T: (static member (+): ^T * ^T -> ^T)
            and ^T: (static member DivideByInt: ^T * int -> ^T)
            and ^T: (static member Zero: ^T)

    /// <summary>Returns the average of the elements generated by applying the function to each element of the immarray.</summary>
    ///
    /// <param name="projection">The function to transform the immarray elements before averaging.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <exception cref="T:System.ArgumentException">Thrown when <c>array</c> is empty.</exception>
    ///
    /// <returns>The computed average.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="average-by-1">
    /// <code lang="fsharp">
    /// type Foo = { Bar: float }
    ///
    /// let input = [| {Bar = 2.0}; {Bar = 4.0} |]
    ///
    /// input |> immarray.averageBy (fun foo -> foo.Bar)
    /// </code>
    /// Evaluates to <c>3.0</c>
    /// </example>
    ///
    /// <example id="average-by-2">
    /// <code lang="fsharp">
    /// type Foo = { Bar: float }
    ///
    /// let input : Foo immarray = [| |]
    ///
    /// input |> immarray.averageBy (fun foo -> foo.Bar)
    /// </code>
    /// Throws <c>ArgumentException</c>
    /// </example>
    [<CompiledName("AverageBy")>]
    val inline averageBy:
        projection: ('T -> ^U) -> array: 'T immarray -> ^U
            when ^U: (static member (+): ^U * ^U -> ^U)
            and ^U: (static member DivideByInt: ^U * int -> ^U)
            and ^U: (static member Zero: ^U)

    /// <summary>For each element of the immarray, applies the given function. Concatenates all the results and return the combined immarray.</summary>
    ///
    /// <param name="mapping">The function to create sub-arrays from the input immarray elements.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The concatenation of the sub-arrays.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="collect-1">
    /// <code lang="fsharp">
    /// type Foo = { Bar: int immarray }
    ///
    /// let input = [| {Bar = [| 1; 2 |]}; {Bar = [| 3; 4 |]} |]
    ///
    /// input |> immarray.collect (fun foo -> foo.Bar)
    /// </code>
    /// Evaluates to <c>[| 1; 2; 3; 4 |]</c>
    /// </example>
    ///
    /// <example id="collect-2">
    /// <code lang="fsharp">
    /// let input = [[1; 2]; [3; 4]]
    ///
    /// input |> immarray.collect id
    /// </code>
    /// Evaluates to <c>[| 1; 2; 3; 4 |]</c>
    /// </example>
    [<CompiledName("Collect")>]
    val collect: mapping: ('T -> 'U immarray) -> array: 'T immarray -> 'U immarray

    /// <summary>Compares two immarrays using the given comparison function, element by element.</summary>
    ///
    /// <param name="comparer">A function that takes an element from each immarray and returns an int.
    /// If it evaluates to a non-zero value iteration is stopped and that value is returned.</param>
    /// <param name="array1">The first input immarray.</param>
    /// <param name="array2">The second input immarray.</param>
    ///
    /// <returns>Returns the first non-zero result from the comparison function. If the first immarray has
    /// a larger element, the return value is always positive. If the second immarray has a larger
    /// element, the return value is always negative. When the elements are equal in the two
    /// immarrays, 1 is returned if the first immarray is longer, 0 is returned if they are equal in
    /// length, and -1 is returned when the second immarray is longer.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when either of the input immarrays
    /// is null.</exception>
    ///
    /// <example id="compare-with-1">
    /// <code lang="fsharp">
    /// let closerToNextDozen a b =
    ///   (a % 12).CompareTo(b % 12)
    ///
    /// let input1 = [| 1; 10 |]
    /// let input2 = [| 1; 10 |]
    ///
    /// (input1, input2) ||> immarray.compareWith closerToNextDozen
    /// </code>
    /// Evaluates to <c>0</c>
    /// </example>
    ///
    /// <example id="compare-with-2">
    /// <code lang="fsharp">
    /// let closerToNextDozen a b =
    ///   (a % 12).CompareTo(b % 12)
    ///
    /// let input1 = [| 1; 5 |]
    /// let input2 = [| 1; 8 |]
    ///
    /// (input1, input2) ||> immarray.compareWith closerToNextDozen
    /// </code>
    /// Evaluates to <c>-1</c>
    /// </example>
    ///
    /// <example id="compare-with-3">
    /// <code lang="fsharp">
    /// let closerToNextDozen a b =
    ///   (a % 12).CompareTo(b % 12)
    ///
    /// let input1 = [| 1; 11 |]
    /// let input2 = [| 1; 13 |]
    ///
    /// (input1, input2) ||> immarray.compareWith closerToNextDozen
    /// </code>
    /// Evaluates to <c>1</c>
    /// </example>
    ///
    /// <example id="compare-with-4">
    /// <code lang="fsharp">
    /// let closerToNextDozen a b =
    ///   (a % 12).CompareTo(b % 12)
    ///
    /// let input1 = [| 1; 2 |]
    /// let input2 = [| 1 |]
    ///
    /// (input1, input2) ||> immarray.compareWith closerToNextDozen
    /// </code>
    /// Evaluates to <c>1</c>
    /// </example>
    ///
    /// <example id="compare-with-5">
    /// <code lang="fsharp">
    /// let closerToNextDozen a b =
    ///   (a % 12).CompareTo(b % 12)
    ///
    /// let input1 = [| 1 |]
    /// let input2 = [| 1; 2 |]
    ///
    /// (input1, input2) ||> immarray.compareWith closerToNextDozen
    /// </code>
    /// Evaluates to <c>-1</c>
    /// </example>
    [<CompiledName("CompareWith")>]
    val inline compareWith: comparer: ('T -> 'T -> int) -> array1: 'T immarray -> array2: 'T immarray -> int

    /// <summary>Builds a new immarray that contains the elements of each of the given sequence of immarrays.</summary>
    ///
    /// <param name="arrays">The input sequence of immarrays.</param>
    ///
    /// <returns>The concatenation of the sequence of input immarrays.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input sequence is null.</exception>
    ///
    /// <example id="concat-1">
    /// <code lang="fsharp">
    /// let inputs = [ [| 1; 2 |]; [| 3 |]; [| 4; 5 |] ]
    ///
    /// inputs |> immarray.concat
    /// </code>
    /// Evaluates to <c>[| 1; 2; 3; 4; 5 |]</c>
    /// </example>
    [<CompiledName("Concat")>]
    val concat: arrays: seq<'T immarray> -> 'T immarray

    /// <summary>Tests if the immarray contains the specified element.</summary>
    ///
    /// <param name="value">The value to locate in the input immarray.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>True if the input immarray contains the specified element; false otherwise.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="contains-1">
    /// <code lang="fsharp">
    /// [| 1; 2 |] |> immarray.contains 2 // evaluates to true
    /// [| 1; 2 |] |> immarray.contains 5 // evaluates to false
    /// </code>
    /// </example>
    [<CompiledName("Contains")>]
    val inline contains: value: 'T -> array: 'T immarray -> bool when 'T: equality

    /// <summary>Builds a new immarray that contains the elements of the given immarray.</summary>
    ///
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>A copy of the input immarray.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="copy-1">
    /// <code lang="fsharp">
    /// let source = [| 12; 13; 14 |]
    ///
    /// immarray.copy source
    /// </code>
    /// Evaluates to a new immarray containing<c>[| 12; 13; 14 |]</c>.
    /// </example>
    [<CompiledName("Copy")>]
    val copy: array: 'T immarray -> 'T immarray

    /// <summary>Applies a key-generating function to each element of an immarray and returns an immarray yielding unique
    /// keys and their number of occurrences in the original immarray.</summary>
    ///
    /// <param name="projection">A function transforming each item of the input immarray into a key to be
    /// compared against the others.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The result immarray.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="count-by-1">
    /// <code lang="fsharp">
    /// type Foo = { Bar: string }
    ///
    /// let inputs = [| {Bar = "a"}; {Bar = "b"}; {Bar = "a"} |]
    ///
    /// inputs |> immarray.countBy (fun foo -> foo.Bar)
    /// </code>
    /// Evaluates to <c>[| ("a", 2); ("b", 1) |]</c>
    /// </example>
    [<CompiledName("CountBy")>]
    val countBy: projection: ('T -> 'Key) -> array: 'T immarray -> ('Key * int) immarray when 'Key: equality

    /// <summary>Creates an immarray whose elements are all initially the given value.</summary>
    ///
    /// <param name="count">The length of the immarray to create.</param>
    /// <param name="value">The value for the elements.</param>
    ///
    /// <returns>The created immarray.</returns>
    ///
    /// <exception cref="T:System.ArgumentException">Thrown when count is negative.</exception>
    ///
    /// <example id="create-1">
    /// <code lang="fsharp">
    /// immarray.create 4 "a"
    /// </code>
    /// Evaluates to a new immarray containing<c>[| "a"; "a"; "a"; "a" |]</c>.
    /// </example>
    ///
    /// <example id="create-2">
    /// <code lang="fsharp">
    /// let cell = ref "a"
    /// let immarray = immarray.create 2 cell
    /// cell.Value &lt;- "b"
    ///
    /// </code>
    /// Before evaluation of the last line, <c>array</c> contains<c>[| { contents = "a"}; { contents = "a"} |]</c>.
    /// After evaluation of the last line <c>array</c> contains<c>[| { contents = "b"}; { contents = "b"} |]</c>.
    /// Note each entry in the immarray is the same mutable cell object.
    /// </example>
    [<CompiledName("Create")>]
    val create: count: int -> value: 'T -> 'T immarray

    /// <summary>Returns the first element of the immarray, or
    /// <c>None</c> if the immarray is empty.</summary>
    ///
    /// <param name="array">The input immarray.</param>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <returns>The first element of the immarray or None.</returns>
    ///
    /// <example id="tryhead-1">
    /// <code lang="fsharp">
    /// let inputs = [| "banana"; "pear" |]
    ///
    /// inputs |> immarray.tryHead
    /// </code>
    /// Evaluates to <c>Some "banana"</c>
    /// </example>
    ///
    /// <example id="tryhead-2">
    /// <code lang="fsharp">
    /// let inputs : int immarray = [| |]
    ///
    /// inputs |> immarray.tryHead
    /// </code>
    /// Evaluates to <c>None</c>
    /// </example>
    [<CompiledName("TryHead")>]
    val tryHead: array: 'T immarray -> 'T option

    /// <summary>Applies the given function to successive elements, returning the first
    /// result where the function returns <c>Some(x)</c> for some <c>x</c>. If the function
    /// never returns <c>Some(x)</c> then <c>None</c> is returned.</summary>
    ///
    /// <param name="chooser">The function to transform the immarray elements into options.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The first transformed element that is <c>Some(x)</c>.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="trypick-1">
    /// <code lang="fsharp">
    /// let input = [| 1; 2; 3 |]
    ///
    /// input |> immarray.tryPick (fun n -> if n % 2 = 0 then Some (string n) else None)
    /// </code>
    /// Evaluates to <c>Some "2"</c>.
    /// </example>
    ///
    /// <example id="trypick-2">
    /// <code lang="fsharp">
    /// let input = [| 1; 2; 3 |]
    ///
    /// input |> immarray.tryPick (fun n -> if n > 3 = 0 then Some (string n) else None)
    /// </code>
    /// Evaluates to <c>None</c>.
    /// </example>
    ///
    [<CompiledName("TryPick")>]
    val tryPick: chooser: ('T -> 'U option) -> array: 'T immarray -> 'U option

    /// <summary>Fills a range of elements of the immarray with the given value.</summary>
    ///
    /// <param name="target">The target immarray.</param>
    /// <param name="targetIndex">The index of the first element to set.</param>
    /// <param name="count">The number of elements to set.</param>
    /// <param name="value">The value to set.</param>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    /// <exception cref="T:System.ArgumentException">Thrown when either targetIndex or count is negative.</exception>
    ///
    /// <example id="fill-1">
    /// <code lang="fsharp">
    /// let target = [| 0; 1; 2; 3; 4; 5 |]
    ///
    /// immarray.fill target 3 2 100
    /// </code>
    /// After evaluation <c>target</c> contains <c>[| 0; 1; 2; 100; 100; 5 |]</c>.
    /// </example>
    [<CompiledName("Fill")>]
    val fill: target: 'T immarray -> targetIndex: int -> count: int -> value: 'T -> 'T immarray

    /// <summary>Applies the given function to successive elements, returning the first
    /// result where the function returns <c>Some(x)</c> for some <c>x</c>. If the function
    /// never returns <c>Some(x)</c> then <see cref="T:System.Collections.Generic.KeyNotFoundException"/> is raised.</summary>
    ///
    /// <param name="chooser">The function to generate options from the elements.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    /// <exception cref="T:System.Collections.Generic.KeyNotFoundException">Thrown if every result from
    /// <c>chooser</c> is <c>None</c>.</exception>
    ///
    /// <returns>The first result.</returns>
    ///
    /// <example id="pick-1">
    /// <code lang="fsharp">
    /// let input = [| 1; 2; 3 |]
    ///
    /// input |> immarray.pick (fun n -> if n % 2 = 0 then Some (string n) else None)
    /// </code>
    /// Evaluates to <c>"2"</c>.
    /// </example>
    ///
    /// <example id="pick-2">
    /// <code lang="fsharp">
    /// let input = [| 1; 2; 3 |]
    ///
    /// input |> immarray.pick (fun n -> if n > 3 = 0 then Some (string n) else None)
    /// </code>
    /// Throws <c>KeyNotFoundException</c>.
    /// </example>
    ///
    [<CompiledName("Pick")>]
    val pick: chooser: ('T -> 'U option) -> array: 'T immarray -> 'U

    /// <summary>Applies the given function to each element of the immarray. Returns
    /// the immarray comprised of the results <c>x</c> for each element where
    /// the function returns <c>Some(x)</c></summary>
    ///
    /// <param name="chooser">The function to generate options from the elements.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The immarray of results.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
    ///
    /// <example id="choose-1">
    /// <code lang="fsharp">
    /// let input = [| Some 1; None; Some 2 |]
    ///
    /// input |> immarray.choose id
    /// </code>
    /// Evaluates to <c>[| 1; 2 |]</c>
    /// </example>
    ///
    /// <example id="choose-2">
    /// <code lang="fsharp">
    /// let input = [| 1; 2; 3 |]
    ///
    /// input |> immarray.choose (fun n -> if n % 2 = 0 then Some n else None)
    /// </code>
    /// Evaluates to <c>[| 2 |]</c>
    /// </example>
    [<CompiledName("Choose")>]
    val choose: chooser: ('T -> 'U option) -> array: 'T immarray -> 'U immarray

    /// <summary>Divides the input immarray into chunks of size at most <c>chunkSize</c>.</summary>
    ///
    /// <param name="chunkSize">The maximum size of each chunk.</param>
    /// <param name="array">The input immarray.</param>
    ///
    /// <returns>The immarray divided into chunks.</returns>
    ///
    /// <exception cref="T:System.ArgumentNullException">Thrown when the input immarray is null.</exception>
